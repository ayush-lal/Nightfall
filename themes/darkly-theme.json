  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#8394b8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#03c1b6"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#5CE6AD"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#5CE6AD",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#5CE6AD"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FFAE57",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#03c1b6"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "Variable names",
      "scope": [
        "entity.name.variable",
        "variable",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#ee7b79"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.other.readwrite",
        "variable.other.readwrite.alias",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#6CD3E8"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#4090A1",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#4090A1"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FAB795B3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#5CE6AD"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#B877DBE6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#03c1b6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "Storage keywords",
      "scope": "storage",
      "settings": {
        "foreground": "#4090A1",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Strings",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#BAE67E"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "foreground": "#5CE6AD"
      }
    },
    {
      "name": "Provided functions",
      "scope": ["support.function", "meta.export.default"],
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#5CE6AD"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#C3A6FF"
      }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#5CE6AD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#A2AABC",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Key/value separators",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#FFAE57"
      }
    }
  ]
}
